<table class="list-videos">
  <tbody>
    <% @vid_array.each do |vid| %>
      <tr>
        <td>
          <a href="/videos/<%= vid.id %>"><%= vid.title %></a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= render partial: "comments/comment_form", locals: {location: @list_id, type: "playlists"} %>
<%= render partial: "comments/comments_table", locals: {comments: @comments} %>